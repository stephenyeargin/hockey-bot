{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0
  },
  "plugins": [
    "eslint-plugin-jest",
    "eslint-plugin-import",
    "eslint-plugin-jest",
    "eslint-plugin-n",
    "eslint-plugin-promise"
  ]
}
